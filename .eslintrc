{
  "extends": ["airbnb", "airbnb/hooks"],
  "parser": "@babel/eslint-parser",
  "globals": {
    "chrome": "readonly"
  },
  "env": {
    "browser": true,
    "webextensions": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-debugger": "warn",
    "object-curly-newline": 0,
    "react/destructuring-assignment": 0,
    "react/jsx-one-expression-per-line": 0
  }
}
